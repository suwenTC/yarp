#!/bin/bash

start=$(date +%s)
start_date=date
bash /root/run_queries.sh 1 &
bash /root/run_queries.sh 2 &
bash /root/run_queries.sh 3 &
bash /root/run_queries.sh 4 &
bash /root/run_queries.sh 5

end=$(date +%s)
end_date=date
time_cost=$(( end - start ))
touch /root/time_cost/xin1_overall.txt
echo "$start_date, $end_date, $time_cost" > /root/time_cost/xin1_overall.txt
